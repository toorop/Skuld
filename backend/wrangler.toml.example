# Exemple de configuration Wrangler pour Skuld
# Copiez ce fichier vers wrangler.toml et adaptez les valeurs

name = "skuld-api"
main = "src/index.ts"
compatibility_date = "2025-01-01"

[vars]
# Ces variables peuvent aussi être définies via `wrangler secret put`
# SUPABASE_URL = "https://votre-projet.supabase.co"
# SUPABASE_ANON_KEY = "eyJ..."
# SUPABASE_SERVICE_ROLE_KEY = "eyJ..."
# APP_URL = "https://votre-skuld.pages.dev"

[[r2_buckets]]
binding = "R2_BUCKET"
bucket_name = "skuld-proofs"
